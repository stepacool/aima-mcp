  "scripts": {
    "dev": "dotenv -e .env.local -- sh -c \"NODE_OPTIONS='--import ./instrument.server.mjs' vite dev --port 3000\"",
    "build": "vite build",
    "build:local": "NITRO_PRESET=node vite build && cp instrument.server.mjs .output/server",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
